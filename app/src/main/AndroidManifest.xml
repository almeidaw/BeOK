            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main" />
        <activity android:name=".Cadastro" />
        <activity android:name=".Diary1" />
        <activity android:name=".Diary2" />
        <activity android:name=".Diary3Alcohol" />
        <activity android:name=".Diary3Marijuana" />
        <activity android:name=".Diary3Cocaine" />
