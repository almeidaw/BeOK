        <activity android:name=".Diary5" />
        <activity android:name=".Diary6" />
        <activity android:name=".FirstAccess" />
        <activity android:name=".Login"></activity>

        <meta-data android:name="DATABASE" android:value="beok.db" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="beok.beok.POJO" />
